{
    "expo": {
        "name": "SafiraJobs",
        "slug": "safirajobs",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/icon.png",
        "scheme": "safirajobs",
        "userInterfaceStyle": "automatic",
        "newArchEnabled": true,
        "splash": {
            "image": "./assets/splash-icon.png",
            "resizeMode": "contain",
            "backgroundColor": "#1a1a2e"
        },
        "ios": {
            "supportsTablet": true,
            "bundleIdentifier": "com.safirajobs.app"
        },
        "android": {
            "versionCode": 1,
            "adaptiveIcon": {
                "foregroundImage": "./assets/adaptive-icon.png",
                "backgroundColor": "#1a1a2e"
            },
            "package": "com.safirajobs.app",
            "permissions": [
                "INTERNET",
                "RECEIVE_BOOT_COMPLETED",
                "VIBRATE"
            ]
        },
        "web": {
            "bundler": "metro",
            "output": "static",
            "favicon": "./assets/favicon.png"
        },
        "plugins": [
            "expo-router",
            [
                "expo-notifications",
                {
                    "icon": "./assets/icon.png",
                    "color": "#4FD1C5"
                }
            ]
        ],
        "experiments": {
            "typedRoutes": true
        },
        "extra": {
            "apiUrl": "https://safirajobs.onrender.com/api",
            "eas": {
                "projectId": "your-project-id"
            }
        }
    }
}